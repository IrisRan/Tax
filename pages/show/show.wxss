.container{
  display: flex;
  flex-direction: column;
  height: 100vh;
  align-items: center;
  justify-content: space-between;
}

.center {
  display: inline-block;
  margin-top: 26rpx;
  margin-left: 40rpx;
  margin-right: 40rpx;
  width: 696rpx;
  height: 880rpx;
  background-repeat: no-repeat;
  background-size: cover;
}

.center-top {
  width: 696rpx;
  height: 264rpx;
  margin-top: 56rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.your-tax {
  font-size: 50rpx;
  color: #666666;
  font-weight: bold;
  margin-bottom: 11rpx;
}

.tax {
  font-size: 88rpx;
  color: #99CC33;
  font-weight: bold;
  text-align: center;
}

#panel{
  height:250px;
  background:#fff;
  box-shadow: 0px 0px 50px rgba(7,17,27,0.1);
  border-radius: 4px;
}
#panel text{
  line-height: 20px;
  border-bottom: 1rpx solid #99CC33;
}
.flex-row{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.flex-column{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
  align-content: space-between;
}
.flex-cell{
  flex: 1;
  font-size: 16px;
  color: #666666;
  padding-top: 20px;
  padding-bottom: 20px;
}